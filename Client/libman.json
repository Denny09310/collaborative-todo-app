{
  "version": "3.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "preline@3.2.3",
      "destination": "wwwroot/lib/preline/",
      "files": [
        "preline.min.js"
      ]
    },
    {
      "library": "toastify-js@1.12.0",
      "destination": "wwwroot/lib/toastify/",
      "fileMappings": [
        {
          "root": "src",
          "files": [
            "toastify.css",
            "toastify.min.js"
          ]
        }
      ]
    }
  ]
}